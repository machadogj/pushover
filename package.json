{
    "name" : "pushover",
    "description" : "git push deploy server over http",
    "version" : "1.1.0",
    "repository" : {
        "type" : "git",
        "url" : "git://github.com/substack/pushover.git"
    },
    "main" : "index.js",
    "keywords" : [
        "git",
        "push",
        "deploy",
        "http",
        "web",
        "repository"
    ],
    "directories" : {
        "lib" : ".",
        "example" : "example",
        "test" : "test"
    },
    "scripts" : {
        "test" : "tap test/*.js"
    },
    "dependencies" : {
        "http-duplex" : "~0.0.2",
        "inherits" : "~1.0.0",
        "mkdirp" : "~0.3.4",
        "which":"*"
    },
    "devDependencies" : {
        "tap" : "~0.2.5",
        "seq" : "~0.3.5"
    },
    "engines" : {
        "node" : ">=0.4.0"
    },
    "license" : "MIT",
    "author" : {
        "name" : "James Halliday",
        "email" : "mail@substack.net",
        "url" : "http://substack.net"
    }
}
